<h1><%= @artist[:name] %></h1>
<% if !@artist[:albums].empty? %>
  <h2>Albums</h2>
  <ul>
    <% @artist[:albums].each do |album| %>
      <li><a href="#"><%= album %></a></li>
    <% end %>
  </ul>
<% end %>

<% if !@artist[:single_tracks].empty? %>
  <h2>Single Tracks</h2>
  <ul>
    <% @artist[:single_tracks].each do |track| %>
      <li><a href="#"><%= track %></a></li>
    <% end %>
  </ul>
<% end %>
